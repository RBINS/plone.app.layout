<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

    <browser:page
        for="*"
        name="search"
        class=".view.SearchView"
        permission="zope2.View"
        template="search.pt"
        />
    
    <browser:page
        for="*"
        name="livesearch"
        class=".view.SearchView"
        permission="zope2.View"
        template="livesearch.pt"
        />
</configure>
